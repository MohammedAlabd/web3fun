{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "prettier", "next/core-web-vitals"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": "off", // conflicts with ts imports
    "no-underscore-dangle": "off", // allow _id dangles
    "react/jsx-props-no-spreading": "off",
    "no-use-before-define": "off", // Fix React used before it was defined
    "@typescript-eslint/no-use-before-define": ["error"], // Fix React used before it was defined
    "no-shadow": "off",
    "@typescript-eslint/no-shadow": ["warn"],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "react/require-default-props": "off",
    "import/prefer-default-export": "off",
    "react/function-component-definition": "off", // this rule added to fix an issue with eslint-config-airbnb@19.0.4
    "no-restricted-exports": "off",
    "react/no-unstable-nested-components": "off",
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
    "no-async-promise-executor": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off",
    "no-plusplus": "off",
    "one-var-declaration-per-line": 0,
    "new-cap": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "no-console": 1,
    "indent": "off",
    "comma-dangle": 0,
    "object-curly-newline": "off",
    "curly": ["error", "multi-line"],
    "no-unused-expressions": "warn",
    "import/no-named-as-default": "warn",
    "import/no-dynamic-require": 0,
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2,
        "maxEOF": 1
      }
    ],
    "eol-last": 1,
    "arrow-parens": "off",
    "import/no-unresolved": [
      2,
      {
        "commonjs": true
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
